		"@fortawesome/svelte-fontawesome": "^0.2.2",
		"gsap": "^3.12.5",
		"svelte-meta-tags": "^3.1.4",
		"three": "^0.168.0",
		"tsparticles": "^3.5.0",
		"typeface-orbitron": "^1.1.13"
